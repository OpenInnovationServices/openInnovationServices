    <h2>No pictures to show at the moment</h2>
    <% }else{ %>
    <div class="row">
            <% for(var i=0; i < data.length; i++) { %>
        <div class="col-lg-4 col-md-6 col-sm-12" style="text-align: center;">
                <img src="/gallery/img/<%= data[i]._id %>" class="rounded" alt="<%= data[i].imgText %>" width="350" height="236">  
